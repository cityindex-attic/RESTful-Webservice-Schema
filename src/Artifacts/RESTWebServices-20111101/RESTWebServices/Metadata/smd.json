          "uriTemplate": "/",
          "contentType": "application/json",
          "responseContentType": "application/json",
          "transport": "POST",
          "envelope": "JSON",
          "returns": {
            "$ref": "ApiLogOnResponseDTO"
          },
          "group": "Authentication",
          "cacheDuration": 0,
          "throttleScope": "data",
          "parameters": [
            {
              "$ref": "#/ApiLogOnRequestDTO",
              "name": "apiLogOnRequest",
              "description": "The request to create a session (log on)."
