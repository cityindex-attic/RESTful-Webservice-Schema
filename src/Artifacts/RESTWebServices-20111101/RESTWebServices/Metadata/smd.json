            {
              "type": "integer",
              "name": "clientAccountId",
              "description": "The logged on user's ClientAccountId. (This only shows you markets that you can trade on.)",
              "demoValue": 123456,
              "minimum": 1,
              "maximum": 84272157
            },
            {
              "type": "integer",
              "name": "maxResults",
              "description": "The maximum number of markets to return.",
              "demoValue": 20,
              "minimum": 1,
              "maximum": 500,
              "optional": true,
              "default": 20
            }
          ]
